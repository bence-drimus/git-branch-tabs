{
  "name": "git-branch-tabs",
  "displayName": "Git Branch Tabs",
  "description": "An extension to manage open tabs based on the current Git branch.",
  "version": "0.0.1",
  "publisher": "bence-drimus",
  "repository": {
    "type": "git",
    "url": "https://github.com/bence-drimus/git-branch-tabs"
  }, 
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:git-branch-tabs.syncTabs"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-branch-tabs.syncTabs",
        "title": "Sync Current Tabs with Branch"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  },
  "extensionDependencies": [
    "vscode.git"
  ]
}
